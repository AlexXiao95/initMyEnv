[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[user]
	name = #NAME#
	email = #MAIL#
[diff]
	tool = vimdiff
[difftool]
	prompt = true
[merge]
	tool = vimdiff
[mergetool]
	prompt = false
	keepBackup = false
[help]
	autocorrect = 1
[color]
	ui = true
[core]
	editor = /usr/bin/vim
	ignorecase = false
